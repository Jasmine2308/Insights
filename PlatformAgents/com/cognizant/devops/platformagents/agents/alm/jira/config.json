          "name": "issueType"
        },
        "updated": "lastUpdated",
        "project": {
          "name": "projectName"
        },
        "creator": {
          "name": "creator"
        },
        "priority": {
          "name": "priority"
        },
        "assignee": {
          "name": "assignee"
        },
        "status": {
          "name": "status"
        },
        "attachment": [{
          "content": "Attachment"
        }],
