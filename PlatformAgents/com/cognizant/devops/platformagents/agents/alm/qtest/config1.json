{
  "mqConfig": {
    "user": "iSight",
    "password": "iSight",
    "host": "127.0.0.1",
    "exchange": "iSight",
    "agentControlXchg": "iAgent"
  },
  "subscribe": {
    "config": "ALM.QTEST.config"
  },
  "publish": {
    "data": "ALM.QTEST.DATA",
    "health": "ALM.QTEST.HEALTH"
  },
  "communication": {
    "type": "REST",
    "sslVerify": false,
    "responseType": "JSON"
  },
  "dynamicTemplate": {
    "timeFieldMapping": {
      "creationDate": "%Y-%m-%dT%H:%M:%S",
      "lastModifiedDate": "%Y-%m-%dT%H:%M:%S",
      "automationTime": "%Y-%m-%dT%H:%M:%S"
    },
    "almEntities": {
      "requirements": {
        "name": "name",
        "pid": "pid",
        "id": "id",
        "created_date": "creationDate",
        "last_modified_date": "lastModifiedDate",
        "description": "description"
      },
      "test-cases": {
        "name": "name",
        "pid": "pid",
        "id": "id",
        "created_date": "creationDate",
        "last_modified_date": "lastModifiedDate"
      },
      "defects": {
        "name": "name",
        "pid": "pid",
        "id": "id",
        "created_date": "creationDate",
        "last_modified_date": "lastModifiedDate",
        "description": "description"
      },
      "test-runs": {
        "name": "name",
        "pid": "pid",
        "id": "id",
        "created_date": "creationDate",
        "last_modified_date": "lastModifiedDate",
        "description": "description"
      }
    },
    "almEntityMetaData": {
      "dataUpdateSupported": true,
      "uniqueKey": [
        "projectId",
        "id",
        "almType"
      ]
    },
    "automationType": {
      "test-cases": {
        "field": "Type",
        "newValue": "Automation"
      }
    },
    "testRunsType": {
      "testRunsWithAutomation": {
        "query": "'Test Case Type' = 'Automation'",
        "automation": true
      },
      "testRunsWithOutAutomation": {
        "query": "'Test Case Type' <> 'Automation'",
        "automation": false
      }
    },
    "extensions": {
      "linkedArtifacts": {
        "runSchedule": 60,
        "almEntityMetaData": {
          "dataUpdateSupported": true,
          "uniqueKey": [
            "projectId",
            "id",
            "almType",
            "almParentType"
          ]
        },
        "almEntities": {
          "test-cases": {
          },
          "requirements": {
          },
          "defects": {
          },
          "test-runs": {
          }
        }
      }
    }
  },
  "toolCategory": "ALM",
  "toolsTimeZone": "GMT",
  "insightsTimeZone": "GMT",
  "enableValueArray": false,
  "useResponseTemplate": true,
  "isHistoryApi": false,
  "historyIdChunkSize": 10,
  "responsePageSize": 99,
  "auth": "base64",
  "runSchedule": 30,
  "timeStampField": "lastModifiedDate",
  "timeStampFormat": "%Y-%m-%dT%H:%M:%S",
  "isEpochTimeFormat": false,
  "startFrom": "2019-01-01T00:00:00",
  "baseUrl": "baseUrl",
  "username": "username",
  "password": "password",
  "isDebugAllowed": false,
  "loggingSetting": {
    "logLevel": "WARN",
    "maxBytes": 5000000,
    "backupCount": 1000
  }
}