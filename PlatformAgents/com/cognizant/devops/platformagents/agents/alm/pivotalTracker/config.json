{
  "mqConfig": {
    "user": "iSight",
    "password": "iSight",
    "host": "127.0.0.1",
    "exchange": "iSight",
    "agentControlXchg": "iAgent"
  },
  "subscribe": {
    "config": "ALM.PIVOTALTRACKER.config"
  },
  "publish": {
    "data": "ALM.PIVOTALTRACKER.DATA",
    "health": "ALM.PIVOTALTRACKER.HEALTH"
  },
  "communication": {
    "type": "REST",
    "sslVerify": false,
    "responseType": "JSON"
  },
  "dynamicTemplate": {
    "activityResponseTemplate":{
      "kind": "kind",
      "message": "message",
      "occurred_at": "occurredAt",
      "performed_by": {
        "name" : "name",
        "id" : "performedById"
      }
    },
    "activityRelationMetadata": {
      "labels": [
        "LATEST"
      ],
      "relation": {
        "properties": [
            "nodeType",
            "key",
            "projectId",
            "attachedCommitId",
            "attachedScmType",
            "attachedCommitMessage"
        ],
        "name": "STORY_HAS_ACTIVITY",
        "source": {
          "constraints": [
          	"projectId",
            "attachedCommitId",
            "attachedScmType",
            "attachedCommitMessage"
          ]
        },
        "destination": {
          "constraints": [
            "key"
          ]
        }
      }
    },
    "iterationResponseTemplate": {
      "number": "iterationNumber",
      "start": "iterationStartTime",
      "finish": "iterationFinishTime",
      "project_id": "projectId",
      "length": "iterationLengthInWeek",
      "velocity": "velocity",
      "analytics": {
        "cycle_time": "cycleTime",
        "rejection_rate": "rejectionRate"
      }
    },
    "timeFieldMapping": {
      "startDate": "%Y-%m-%d"
    },
    "responseTemplate": {
      "id": "key",
      "created_at": "createdAt",
      "story_type": "storyType",
      "name": "storyName",
      "current_state": "currentStoryState",
      "estimate": "storyPoint",
      "requested_by_id": "requesterId",
      "owned_by_id": "ownerId",
      "project_id": "projectId",
      "url": "url"
    },
    "relationMetadata": {
      "labels": [
        "LATEST"
      ],
      "relation": {
        "properties": [
          "iterationNumber",
          "projectId",
          "key",
          "backLog",
          "cycleTime",
          "rejectionRate",
          "iterationStartTime",
          "iterationFinishTime",
          "iterationLengthInWeek"
        ],
        "name": "ITERATION_HAS_ISSUES",
        "source": {
          "constraints": [
            "projectId",
            "key"
          ]
        },
        "destination": {
          "constraints": [
            "iterationNumber"
          ]
        }
      }
    },
    "storyMetadata": {
      "labels": [
        "STORY"
      ],
      "dataUpdateSupported": true,
      "uniqueKey": [
        "projectId",
        "key"
      ]
    }
  },
  "agentId" : "",
  "getAllActivity": true,
  "getAllMembers": true,
  "auth": "base64",
  "runSchedule": 30,
  "toolCategory": "ALM",
  "enableValueArray": false,
  "enableDataValidation": true,
  "useResponseTemplate": true,
  "userid": "email_user_name",
  "passwd": "password",
  "token": "access_token",
  "baseEndPoint": "https://www.pivotaltracker.com",
  "startFrom": "2019-01-01 12:17:45",
  "toolsTimeZone": "Asia/Kolkata",
  "timeStampField": "createdAt",
  "timeStampFormat": "%Y-%m-%dT%H:%M:%SZ",
  "isEpochTimeFormat": false,
  "isDebugAllowed": true,
  "loggingSetting": {
    "logLevel": "WARN",
    "maxBytes": 5000000,
    "backupCount": 1000
  }
}